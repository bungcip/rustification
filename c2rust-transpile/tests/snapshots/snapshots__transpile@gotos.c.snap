use core::ffi;
#[unsafe(no_mangle)]
pub unsafe extern "C" fn sum(mut count: ffi::c_int) -> ffi::c_int {
    let mut x: ffi::c_int = 0 as ffi::c_int;
    while !(count <= 0 as ffi::c_int) {
        x += count;
        count -= 1;
        count;
    }
    return x;
}
